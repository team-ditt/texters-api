    "@aws-sdk/lib-storage": "^3.367.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.5",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.5",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/platform-express": "^10.0.5",
    "@nestjs/typeorm": "^10.0.0",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "googleapis": "^120.0.0",
    "pg": "^8.11.1",
    "ramda": "^0.29.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.32.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.7",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.5",
    "@types/cookie-parser": "^1.4.3",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.7",
